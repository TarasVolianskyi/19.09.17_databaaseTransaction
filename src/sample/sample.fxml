<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" vgap="10"
          xmlns="http://javafx.com/javafx/8.0.121" fx:controller="sample.Controller">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="200" minWidth="10.0" prefWidth="130.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="200" minWidth="10.0" prefWidth="130.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="200" minWidth="10.0" prefWidth="130.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="200" minWidth="10.0" prefWidth="130.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Create" textAlignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label text="Read" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label text="Update" GridPane.columnIndex="2" GridPane.rowIndex="0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label text="Delte" GridPane.columnIndex="3" GridPane.rowIndex="0">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>

                <Button fx:id="btnDeleteAll" mnemonicParsing="false" onAction="#doDeleteAll" text="Delete All"
                        GridPane.columnIndex="3" GridPane.rowIndex="9"/>
                <Button fx:id="btnDeleteWithLimit" mnemonicParsing="false" onAction="#doDeleteWithLimit"
                        text="Delete With Limit"
                        GridPane.columnIndex="3" GridPane.rowIndex="8"/>
                <Button fx:id="btnPrint" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                        onAction="#doPrint" text="Print all users" textAlignment="CENTER" GridPane.columnIndex="1"
                        GridPane.rowIndex="2"/>
                <Button fx:id="btnPrintWithLimit" mnemonicParsing="false" onAction="#doPrintWithLimit"
                        text="Print with limit" GridPane.columnIndex="1" GridPane.rowIndex="9"/>
                <Button fx:id="btnSafeStart" mnemonicParsing="false" onAction="#doSafeStart" text="Safe Start"
                        GridPane.columnIndex="0" GridPane.rowIndex="8"/>
                <Button fx:id="btnUpdate" mnemonicParsing="false" onAction="#doUpdate" text="Update"
                        GridPane.columnIndex="2" GridPane.rowIndex="9"/>
                <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#doDelete" text="Delete"
                        GridPane.columnIndex="3" GridPane.rowIndex="7"/>
                <Label text="Name" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
                <TextField fx:id="txtFieldCName" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
                <Label text="Password" GridPane.columnIndex="0" GridPane.rowIndex="5"/>
                <TextField fx:id="txtFieldCPass" GridPane.columnIndex="0" GridPane.rowIndex="6"/>
                <Label text="From" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <TextField fx:id="txtFieldRFrom" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <Label text="To" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                <TextField fx:id="txtFieldRTo" GridPane.columnIndex="1" GridPane.rowIndex="7"/>

                <Label text="ID for Update" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
                <TextField fx:id="txtFieldUID" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
                <Label text="Name" GridPane.columnIndex="2" GridPane.rowIndex="4"/>
                <TextField fx:id="txtFieldUName" GridPane.columnIndex="2" GridPane.rowIndex="5"/>
                <Label text="Password" GridPane.columnIndex="2" GridPane.rowIndex="6"/>
                <TextField fx:id="txtFieldUPass" GridPane.columnIndex="2" GridPane.rowIndex="7"/>

                <Label text="ID for Delete" GridPane.columnIndex="3" GridPane.rowIndex="2"/>
                <TextField fx:id="txtFieldDID" GridPane.columnIndex="3" GridPane.rowIndex="3"/>
                <Label text="From - To" GridPane.columnIndex="3" GridPane.rowIndex="4"/>
                <TextField fx:id="txtFieldDIDTo" GridPane.columnIndex="3" GridPane.rowIndex="5"/>
            </children>
        </GridPane>
    </children>
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
